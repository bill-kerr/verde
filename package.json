		"@sveltestack/svelte-query": "^1.4.1",
		"axios": "^0.21.1",
		"clsx": "^1.1.1",
		"date-fns": "^2.23.0",
		"dotenv": "^10.0.0",
		"firebase": "^8.9.0",
		"firebase-admin": "^9.11.1",
		"tippy.js": "^6.3.1",
		"yup": "^0.32.9"
	}
}
